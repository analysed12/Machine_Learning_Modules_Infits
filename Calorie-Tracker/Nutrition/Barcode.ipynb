   "outputs": [],
   "source": [
    "def BAR(location):\n",
    "    myData=False\n",
    "    img = cv2.imread(location)\n",
    "    for barcode in decode(img):\n",
    "        myData = barcode.data.decode('utf-8')\n",
    "    return myData"
   ]
  }
